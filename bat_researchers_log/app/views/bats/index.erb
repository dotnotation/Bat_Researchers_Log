<h1>Welcome 
    <% if logged_in? %>
        <%= current_user.username %>
        <% end %> </h1>
<h2>Recorded Bats:</h2>
    <ol>
        <% @bats.each do |bat| %>
            <li><a href="/bats/<%= bat.identification %>"><%= bat.identification %></a></li>
        <% end %>
    </ol>

<a class="button" href="/">Home</a>
<a class="button" href="/bats/new">Log New Bat</a>